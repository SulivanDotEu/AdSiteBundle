{% extends  'WalvaCrudAdminBundle:Crud:show.html.twig' %}

{% block crud_table_body -%}
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Creationdate</th>
                <td>{{ entity.creationDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Editiondate</th>
                <td>{{ entity.editionDate|date('Y-m-d H:i:s') }}</td>
            </tr>
{% endblock %}
